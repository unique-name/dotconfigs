alias f='find . -name'
alias cd..='cd ..'
alias diff='colordiff'
alias size='sudo du -h --max-depth=0'
alias disk='df -hT'
alias g='grep -rn --exclude=tags --exclude=*.{o,a,i,so,map,T}'
alias gi='grep -rni --exclude=tags --exclude=*.{o,a,i,so,map}'
alias tags='/usr/bin/ctags -R --languages=C,C++ --exclude=*.so,*.o'
alias ll='ls -la --color=auto'
